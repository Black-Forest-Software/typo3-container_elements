{
    "name": "buepro/typo3-container-elements",
    "type": "typo3-cms-extension",
    "description": "TYPO3 extension providing content elements powered by container and bootstrap. Available elements: container, columns, tabs, accordion, tile unit and card.",
    "keywords": [
        "TYPO3 CMS",
        "container",
        "grid",
        "structure",
        "element",
        "pizpalue"
    ],
    "homepage": "https://github.com/buepro/typo3-container_elements",
    "authors": [
        {
            "name": "Roman BÃ¼chler",
            "email": "rb@buechler.pro",
            "homepage": "https://www.buechler.pro/",
            "role": "Developer"
        }
    ],
    "license": ["GPL-2.0-or-later"],
    "require": {
        "typo3/cms-core": "^10.4.11 || 11.*.*@dev",
        "b13/container": "^1.3.0",
        "fluidtypo3/vhs": "^6.0 || dev-development"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Buepro\\ContainerElements\\": "Classes/"
        }
    },
    "replace": {
        "buepro/typo3-container-elements": "self.version",
        "typo3-ter/container-elements": "self.version"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "container_elements"
        },
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
